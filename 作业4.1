# Java
package project1;
public class Dog {
	private String name;
	private String color;
	private String age;
	
	public Dog() {
		this.name="小红";
		this.color="红";
		this.age="5";
	}
	public Dog(String name, String color,String age) {
		this.name=name;
		this.color=color;
		this.age=age;
	}
	public void displayDog() {
		System.out.println("名字："+this.name);
		System.out.println("颜色："+this.color);
		System.out.println("年龄："+this.age);
		
	}
}
